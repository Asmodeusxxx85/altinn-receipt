{
  "name": "receipt-react-app",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "lint": "eslint \"./src/**/*.ts*\"",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@material-ui/core": "4.12.4",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "dompurify": "2.3.9",
    "html-react-parser": "1.4.14",
    "marked": "4.0.18",
    "moment": "2.29.4",
    "react": "17.0.2",
    "react-content-loader": "6.2.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/dompurify": "2.3.3",
    "@types/enzyme": "3.10.12",
    "@types/jest": "28.1.5",
    "@types/jsdom": "16.2.14",
    "@types/marked": "4.0.3",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "altinn-designsystem": "3.1.0",
    "babel-jest": "28.1.3",
    "babel-loader": "8.2.5",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "enzyme": "3.11.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.12",
    "jest": "28.1.3",
    "jest-environment-enzyme": "7.1.2",
    "jest-environment-jsdom": "28.1.3",
    "jest-enzyme": "7.1.2",
    "jest-junit": "13.2.0",
    "jsdom": "20.0.0",
    "mini-css-extract-plugin": "2.6.1",
    "msw": "0.44.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "source-map-loader": "3.0.1",
    "terser-webpack-plugin": "5.3.3",
    "ts-jest": "28.0.6",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "packageManager": "yarn@3.2.1",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^testConfig/(.*)$": "<rootDir>/testConfig/$1"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testConfig/setupTests.ts"
    ],
    "testEnvironmentOptions": {
      "url": "https://localhost/receipt/mockInstanceOwnerId/6697de17-18c7-4fb9-a428-d6a414a797ae"
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "collectCoverageFrom": [
    "!__tests__/**/*",
    "src/**/*.{ts,tsx}"
  ]
}
