{
  "name": "receipt-react-app",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "lint": "eslint \"./src/**/*.ts*\"",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@material-ui/core": "4.12.4",
    "axios": "1.7.7",
    "classnames": "2.5.1",
    "dompurify": "3.1.6",
    "html-react-parser": "5.1.16",
    "marked": "14.1.2",
    "moment": "2.30.1",
    "react": "18.3.1",
    "react-content-loader": "7.0.2",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-react": "7.24.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/dompurify": "3.0.5",
    "@types/jest": "29.5.13",
    "@types/jsdom": "21.1.7",
    "@types/marked": "6.0.0",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "altinn-designsystem": "4.2.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "eslint": "9.11.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.36.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "fork-ts-checker-notifier-webpack-plugin": "9.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jsdom": "25.0.1",
    "mini-css-extract-plugin": "2.9.1",
    "msw": "2.4.9",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "source-map-loader": "5.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "typescript": "4.9.5",
    "undici": "^5.28.3",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "packageManager": "yarn@3.8.5",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^testConfig/(.*)$": "<rootDir>/testConfig/$1"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/testConfig/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testConfig/setupTests.ts"
    ],
    "testEnvironmentOptions": {
      "url": "https://localhost/receipt/mockInstanceOwnerId/6697de17-18c7-4fb9-a428-d6a414a797ae",
      "customExportConditions": [
        ""
      ]
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "collectCoverageFrom": [
    "!__tests__/**/*",
    "src/**/*.{ts,tsx}"
  ]
}
